{
    "name": "association_manager/api",
    "description": "api and admin for advertisement",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.2",
        "aws/aws-sdk-php": "^3.147",
        "cocur/slugify": "^4.0",
        "fzaninotto/faker": "^1.9",
        "knplabs/knp-gaufrette-bundle": "^0.7.1",
        "lexik/jwt-authentication-bundle": "^2.8",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.1.*",
        "symfony/framework-bundle": "5.1.*",
        "symfony/google-mailer": "5.1.*",
        "symfony/http-client": "5.1.*",
        "symfony/intl": "5.1.*",
        "symfony/mailer": "5.1.*",
        "symfony/mime": "5.1.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.1.*",
        "symfony/orm-pack": "*",
        "symfony/process": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/serializer-pack": "*",
        "symfony/string": "5.1.*",
        "symfony/translation": "5.1.*",
        "symfony/twig-pack": "*",
        "symfony/validator": "5.1.*",
        "symfony/web-link": "5.1.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.1.*",
        "symfonycasts/verify-email-bundle": "^1.0",
        "vich/uploader-bundle": "^1.15"
    },
    "require-dev": {
        "behat/behat": "^3.7",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/page-object-extension": "^0.3.1",
        "friends-of-behat/symfony-extension": "^2.1",
        "se/selenium-server-standalone": "^3.141",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "load-data": {
            "deleteDataBaseIfExiste": "php bin/console doctrine:database:drop --force",
            "createDataBase": "php bin/console doctrine:database:create",
            "migration": "php bin/console doctrine:migrations:migrate",
            "loadFixture": "php bin/console doctrine:fixtures:load --append --no-interaction"
        },
        "test": {
            "dropDataBase": "php bin/console doctrine:database:drop --force --env=test",
            "createDataBase": "php bin/console doctrine:database:create --env=test",
            "migration": "php bin/console doctrine:migrations:migrate --env=test",
            "loadFixture": "php bin/console doctrine:fixtures:load --append --no-interaction --env=test",
            "runTest":"phpdbg -d memory_limit=-1 -qrr bin/phpunit --coverage-html var/log"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
